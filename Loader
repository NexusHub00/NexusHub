local Games = {
    [12931609417] = loadstring(game:HttpGet("https://raw.githubusercontent.com/NexusHub00/NexusHub/refs/heads/main/ColorORDie"))
}

-- The loader just finds and calls the function
local scriptFunction = Games[game.PlaceId]

if scriptFunction then
    print("Game Supported! Loading script...")
    scriptFunction() -- Execute the pre-loaded function
else
    local Players = game:GetService("Players")
    local player = Players.LocalPlayer
    player:Kick("Game Not Supported")
end
